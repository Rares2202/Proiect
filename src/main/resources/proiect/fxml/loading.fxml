            fx:controller="proiect.controller.LoadingController"
            prefHeight="400.0" prefWidth="600.0">
    <ImageView fx:id="loadingImage"
               fitHeight="200.0" fitWidth="200.0"
               layoutX="200.0" layoutY="100.0"
               preserveRatio="true"
               pickOnBounds="true" />
</AnchorPane>
