<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<Pane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1">
   <children>

      <TextField fx:id="username_reg"   layoutY="100" layoutX="200"/>
       <Label layoutY="100" layoutX="100" text="Username:" />
       <PasswordField fx:id="password_reg" layoutY="200" layoutX="200"/>
       <Label text="Password:" layoutX="100" layoutY="200"/>
      <CheckBox  fx:id="librarian"  layoutY="300" layoutX="200" mnemonicParsing="false" text="CheckBox" />
      <Label text="Librarian?" layoutY="300" layoutX="100"/>
      <Button fx:id="register" text="Register" layoutY="350" layoutX="200"/>
   </children>
</Pane>
